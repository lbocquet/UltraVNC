#ifndef _MAC
/////////////////////////////////////////////////////////////////////////////
//
// Version
//

#include "version.h"
VS_VERSION_INFO VERSIONINFO
 FILEVERSION MAJOR_INT,MINOR_INT,BUILD_INT,SPECIAL_INT
 PRODUCTVERSION MAJOR_INT,MINOR_INT,BUILD_INT,SPECIAL_INT
